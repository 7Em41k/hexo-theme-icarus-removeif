<% const links = get_config_from_obj(widget, 'item'); %>
<% if (links !== null) { %>
<div class="card widget">
    <div class="card-content">
        <div class="menu">
        <h3 class="menu-label">
            通知
        </h3>
        <ul class="menu-list">
        <% for (let i in links) { %>
            <li>
                    <span class="level-left">
                        <span class="level-item" style="justify-content:left;margin-left:4px;"><%= i %></span>
                    </span>
            </li>
        <% } %>
        </ul>
        </div>
    </div>
</div>
<% } %>
